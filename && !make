#include "src/NumMethods.hpp"

int main() {
    Matrix B(5, 5);
    B << 40,  8,  4,  2, 1,
          8, 30, 12,  6, 2,
          4, 12, 20,  1, 2,
          2,  6,  1, 25, 4,
          1,  2,  2,  4, 5;

    Matrix M1(3, 3);
    M1 << 5, 2, 1,
          2, 3, 1,
          1, 1, 2;

    powerMethod::print(powerMethod::regular(M1, .000001));
    powerMethod::print(powerMethod::inverse(M1, .000001));
    powerMethod::print(powerMethod::shifted(M1, 3, .000001));
}
